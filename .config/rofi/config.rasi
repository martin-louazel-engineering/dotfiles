configuration {
	kb-accept-entry: "Return";
	kb-remove-to-eol: "";
	kb-row-up: "Super+k,Control+k,Up,Control+p";
	kb-row-down: "Super+j,Control+j,Down,Control+n";

	modi: "window,drun,combi";
	combi-modi: "window,drun";
}
@theme "Arc-Dark"

element {
	children: [ element-icon, element-text ];
}

element-icon {
	size: 1.5em;
}
